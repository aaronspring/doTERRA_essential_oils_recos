# doTERRA Essential Oils Recommendations - Environment Variables
# Copy this file to .env and fill in the values

# Qdrant Vector Database Configuration
QDRANT_HOST = localhost
QDRANT_PORT = 6333
QDRANT_COLLECTION = essential_oils

PERPLEXITY_API_KEY=your_perplexity_api_key_here

# Embedding Model Configuration
# MODEL_NAME is hardcoded in config.py, but can be overridden here
# MODEL_NAME=jinaai/jina-embeddings-v2-base-de

# Backend Server Configuration
# HOST=0.0.0.0
# PORT=8000

# Production Environment
# ENVIRONMENT=production

# CORS Configuration
# Comma-separated list of allowed origins for CORS (e.g., "http://localhost:5173,https://example.com")
# ALLOWED_ORIGINS=http://localhost:5173
